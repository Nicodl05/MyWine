{
  "extends": ["config:base"],
  "timezone": "Europe/Paris",
  "labels": ["dependencies"],
  "schedule": ["before 04:00 on monday"],
  "prHourlyLimit": 2,
  "prConcurrentLimit": 5,
  "stabilityDays": 3,
  "automerge": false,
  "packageRules": [
    {
      "matchManagers": ["nuget"],
      "groupName": "All NuGet dependencies",
      "matchPackagePatterns": ["*"]
    }
  ],
  "semanticCommitType": "chore",
  "semanticCommitScope": "deps",
  "rangeStrategy": "bump"
}
